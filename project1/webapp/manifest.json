{
    "_version": "1.49.0",
    "sap.app": {
        "id": "project1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.11.0",
            "toolsId": "fb5ddbeb-86e2-4105-8075-994f0b506180"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.118.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "project1.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
				"routerClass": "sap.f.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "project1.view",
				"controlId": "fcl",
				"transition": "slide",
				"async": true
			},
            "routes": [
                {
                    "name": "main",
                    "pattern": ":layout:",
                    "target": [
                        "main"
                    ]
                },
				{
					"pattern": "detail/{product}/{layout}",
					"name": "detail",
					"target": [
						"main",
						"detail"
					]
				},
				{
					"pattern": "detailDetail/{product}/{supplier}/{layout}",
					"name": "detailDetail",
					"target": [
						"main",
						"detail",
						"detailDetail"
					]
				}
            ],
            "targets": {
                "main": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "main",
                    "viewName": "Main",
                    "controlAggregation": "beginColumnPages"
                },
                "detail": {
                  "viewType": "XML",
                  "transition": "slide",
                  "viewId": "detail",
                  "viewName": "Detail",
                  "controlAggregation": "midColumnPages"
                },
				"detailDetail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "detailDetail",
                    "viewName": "DetailDetail",
					"controlAggregation": "endColumnPages"
				}
            }
        },
        "rootView": {
            "viewName": "project1.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "ProyInt2"
    }
}
